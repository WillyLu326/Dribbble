<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="willy.individual.com.dribbble">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />
        <activity android:name=".views.auth.AuthActivity" />
        <activity android:name=".views.shot_detail.ShotActivity" />
        <activity android:name=".views.shot_imgae_activity.ShotImageActivity" />
        <activity android:name=".views.bucket_list.BucketListActivity" />
        <activity android:name=".views.bucket_list.BucketCrudActivity" />
        <activity android:name=".views.bucket_list.BucketShotListActivity" />
        <activity android:name=".views.profile.ProfileActivity" />

    </application>

</manifest>